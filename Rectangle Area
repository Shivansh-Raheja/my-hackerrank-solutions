
class Rectangle{
   
    public:
     int width,height;
    void display(){
        cout<<width<<" "<<height;
    }
};
class RectangleArea : public Rectangle{
    public:
    void read_input(){
        cin>>width;
        cin>>height;
    }
    void display(){
        cout<<"\n"<<width*height;
        
    }
};
